
ol:Flicker_ID a ol:IdType,
        owl:Class,
        skos:Concept ;
    rdfs:label "Flicker_ID"^^xsd:string ;
    dc:description "Id who has the form of a iRI"@en,
        "a flicker id"@en ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:subClassOf ol:PhotographicId ;
    skos:inScheme ol:idtypes ;
    skos:name "Flicker_ID"@en ;
    skos:narrowerThan ol:PhotographicId .

ol:Foobar_ID a ol:IdType,
        owl:Class,
        skos:Concept ;
    rdfs:label "Foobar_ID"^^xsd:string ;
    dc:description "Id who has the form of a URI"@en,
        "a foo bar id"@en ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
