ol:Region a owl:Class ;
    rdfs:label "Region"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.0/> ;
    rdfs:subClassOf gn:Feature .

ol:Unit a owl:Class ;
    rdfs:label "Unit"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.0/> .

ol:Constraint a owl:Class,
        <https://ns.void/xnode/cardinality-openlicensing-payAmount-0>,
        <https://ns.void/xnode/cardinality-openlicensing-percentage-0>,
        <https://ns.void/xnode/cardinality-openlicensing-product-0>,
        <https://ns.void/xnode/cardinality-openlicensing-spatial-0>,
        <https://ns.void/xnode/cardinality-openlicensing-unit-0> ;
    rdfs:label "Constraint"^^xsd:string ;
    dc:description """
    Changes
    - added leftOperandProperty to give formal semantic linking original usecase / asset with the property checked
    - added leftOperandPropertyPath instead of a leftOperantProperty a properyt path may be used for instance
       "owner/position/country"
    - unit usage is discouraged as they should be specified by left operand property
    """@en ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.0/> ;
    rdfs:subClassOf odrl:Constraint .

