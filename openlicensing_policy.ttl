ol:prohibition a owl:ObjectProperty ;
    monk:embedded true ;
    monk:multiple true ;
    monk:optional false ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range ol:Prohibition ;
    rdfs:subPropertyOf monk:embeddedObjectProperty,
