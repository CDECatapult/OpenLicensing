<https://ns.void/xnode/cardinality-ol-created-0> a owl:Restriction ;
    owl:cardinality 1 ;
    owl:onProperty ol:created .

<https://ns.void/xnode/cardinality-ol-modified-0> a owl:Restriction ;
    owl:cardinality 1 ;
    owl:onProperty ol:modified .

<https://ns.void/xnode/cardinality-openlicensing-defaultAssignee-0> a owl:Restriction ;
    owl:allValuesFrom ol:Party ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:defaultAssignee .

<https://ns.void/xnode/cardinality-openlicensing-defaultAssigner-0> a owl:Restriction ;
    owl:allValuesFrom ol:Party ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:defaultAssigner .

<https://ns.void/xnode/cardinality-openlicensing-defaultTarget-0> a owl:Restriction ;
    owl:allValuesFrom ol:Asset ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:defaultTarget .

<https://ns.void/xnode/cardinality-openlicensing-digitalPolicyAuthor-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:digitalPolicyAuthor .

<https://ns.void/xnode/cardinality-openlicensing-expires-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:expires .

<https://ns.void/xnode/cardinality-openlicensing-permission-0> a owl:Restriction ;
    owl:allValuesFrom ol:Permission ;
    owl:onProperty ol:permission .

<https://ns.void/xnode/cardinality-openlicensing-policyDescription-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:policyDescription .

<https://ns.void/xnode/cardinality-openlicensing-policyHTML-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:policyHTML .

<https://ns.void/xnode/cardinality-openlicensing-policyName-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:policyName .

<https://ns.void/xnode/cardinality-openlicensing-policyText-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:policyText .

<https://ns.void/xnode/cardinality-openlicensing-preferredPolicyIdentifier-0> a owl:Restriction ;
    owl:maxCardinality 1 ;
    owl:onProperty ol:preferredPolicyIdentifier .

<https://ns.void/xnode/cardinality-openlicensing-prohibition-0> a owl:Restriction ;
    owl:allValuesFrom ol:Prohibition ;
    owl:onProperty ol:prohibition .

<https://ns.void/xnode/cardinality-openlicensing-sharedDuty-0> a owl:Restriction ;
    owl:allValuesFrom odrl:Duty ;
    owl:onProperty ol:sharedDuty .

ol:Policy a owl:Class,
        <https://ns.void/xnode/cardinality-ol-created-0>,
        <https://ns.void/xnode/cardinality-ol-modified-0>,
        <https://ns.void/xnode/cardinality-openlicensing-defaultAssignee-0>,
        <https://ns.void/xnode/cardinality-openlicensing-defaultAssigner-0>,
        <https://ns.void/xnode/cardinality-openlicensing-defaultTarget-0>,
        <https://ns.void/xnode/cardinality-openlicensing-digitalPolicyAuthor-0>,
        <https://ns.void/xnode/cardinality-openlicensing-expires-0>,
        <https://ns.void/xnode/cardinality-openlicensing-permission-0>,
        <https://ns.void/xnode/cardinality-openlicensing-policyDescription-0>,
        <https://ns.void/xnode/cardinality-openlicensing-policyHTML-0>,
        <https://ns.void/xnode/cardinality-openlicensing-policyName-0>,
        <https://ns.void/xnode/cardinality-openlicensing-policyText-0>,
        <https://ns.void/xnode/cardinality-openlicensing-preferredPolicyIdentifier-0>,
        <https://ns.void/xnode/cardinality-openlicensing-prohibition-0>,
        <https://ns.void/xnode/cardinality-openlicensing-sharedDuty-0> ;
    rdfs:label "Policy"^^xsd:string ;
    dc:description """
    An ODRL policy representing an agreement or a potential agreement.

    Policy is the central entity that holds an ODRL policy together. In its encoded form, e.g. in an XML document, it makes the policy addressable from the outside word via its uid attribute. Policy can refer to Permissions and Prohibitions.
    """@en ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:subClassOf odrl:Policy .

