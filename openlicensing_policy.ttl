
ol:created a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range xsd:dateTime ;
    rdfs:subPropertyOf dct:created .

ol:defaultAssignee a owl:FunctionalProperty,
        owl:ObjectProperty ;
    monk:embedded false ;
    monk:optional true ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range ol:Party .

ol:defaultAssigner a owl:FunctionalProperty,
        owl:ObjectProperty ;
    monk:embedded false ;
    monk:optional true ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range ol:Party .

ol:defaultTarget a owl:FunctionalProperty,
        owl:ObjectProperty ;
    monk:embedded false ;
    monk:optional true ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range ol:Asset .

ol:digitalPolicyAuthor a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    monk:visibility "public"^^xsd:string ;
    dc:description """
                                 A field to allow the author of the digital policy - if available this person may be
                                 eventually be contacted to obtain infomation about the license.
                                 """@en ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range xsd:string .

ol:expires a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    monk:visibility "public"^^xsd:string ;
    dc:description """
         Optional expiry date of the policy.
         The policy is not considered valid after this date.
         """@en ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range xsd:dateTime .

ol:modified a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range xsd:dateTime ;
    rdfs:subPropertyOf dct:modified .

ol:permission a owl:ObjectProperty ;
    monk:embedded true ;
    monk:multiple true ;
    monk:optional false ;
    monk:visibility "public"^^xsd:string ;
    rdfs:domain ol:Policy ;
    rdfs:isDefinedBy <http://openlicensing.io/ns/ol/1.0/> ;
    rdfs:range ol:Permission ;
    rdfs:subPropertyOf monk:embeddedObjectProperty,
        odrl:permission .

ol:policyDescription a owl:DatatypeProperty,
