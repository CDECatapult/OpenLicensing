op:HubkeyS0 a op:IdType,
        skos:Concept ;
    dc:description "Id who has the form of a Hubkey.  This refers to schema 0 of the Hubkey."@en ;
    skos:inScheme op:id_types ;
    skos:name "HubkeyS0"@en ;
    skos:narrowerThan op:top_id_type .

op:Hubkey a op:IdType,
        skos:Concept ;
    dc:description "Id who has the form of a Hubkey.  This refers to the latest schema of a Hubkey."@en ;
    skos:inScheme op:id_types ;
    skos:name "Hubkey"@en ;
    skos:narrowerThan op:top_id_type .

op:IRI a op:IdType,
        skos:Concept ;
    dc:description "Id which has the form of an IRI"@en ;
    skos:inScheme op:id_types ;
    skos:name "IRI"@en ;
    skos:narrowerThan op:top_id_type .

op:id_type a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain op:Id ;
    rdfs:isDefinedBy <http://openpermissions.org/ns/op/1.1/> ;
    rdfs:range op:IdType .

op:value a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain op:Id ;
    rdfs:isDefinedBy <http://openpermissions.org/ns/op/1.1/> ;
    rdfs:range xsd:string .

op:Id a [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty op:value ],
        [ a owl:Restriction ;
            owl:allValuesFrom op:IdType ;
            owl:cardinality 1 ;
            owl:onProperty op:id_type ],
        owl:Class ;
    rdfs:label "Id"^^xsd:string ;
    dc:description """
Identifier for an asset.
"""@en ;
    rdfs:isDefinedBy <http://openpermissions.org/ns/op/1.1/> .

op:id_types a skos:ConceptScheme ;
    skos:hasTopConcept op:topIdType .

op:topIdType a op:IdType,
        skos:Concept ;
    skos:inScheme op:id_types ;
    skos:name "topIdType"@en .

op:IdType a owl:Class ;
    rdfs:label "IdType"^^xsd:string ;
    dc:description """
Represents a class of identifiers
"""@en ;
    rdfs:isDefinedBy <http://openpermissions.org/ns/op/1.1/> .

