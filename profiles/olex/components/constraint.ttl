odrl:height a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain olex:Constraint ;
    rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf odrl:rightOperand .

odrl:post a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain olex:Constraint ;
    rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf odrl:rightOperand .

odrl:views a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain olex:Constraint ;
    rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf odrl:rightOperand .

odrl:width a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain olex:Constraint ;
    rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf odrl:rightOperand .

olex:Unit a owl:Class ;
    rdfs:label "Unit"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> .

olex:Units a skos:ConceptScheme ;
    owl:deprecated true ;
    skos:ConceptScheme_hasTopConcept olex:all_units .

olex:all_units a olex:Unit,
        skos:Concept ;
    skos:inScheme olex:Units ;
    skos:name "all_units"@en .

olex:Constraint a [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty odrl:height ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty odrl:post ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty odrl:views ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty odrl:width ],
        owl:Class ;
    rdfs:label "Constraint"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf odrl:Constraint .

olex:Purpose a owl:Class ;
    rdfs:label "Purpose"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    owl:deprecated true .

olex:Purposes a skos:ConceptScheme ;
    owl:deprecated true ;
    skos:ConceptScheme_hasTopConcept olex:all_purpose .

olex:all_purpose a olex:Purpose,
        skos:Concept ;
    skos:inScheme olex:Purposes ;
    skos:name "all_purpose"@en .

