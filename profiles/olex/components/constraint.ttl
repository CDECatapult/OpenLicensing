    skos:ConceptScheme_hasTopConcept olex:top_action .


olex:top_action a odrl:Action,
        skos:Concept ;
    skos:inScheme olex:actions ;
    skos:name "top_action"@en .


olex:use a odrl:Action,
        skos:Concept ;
    dc:description """
            The Assigner permits/prohibits the Assignee to use the Asset as agreed.
            """@en ;
    skos:inScheme olex:actions ;
    skos:name "use"@en ;
    skos:narrowerThan olex:top_action .

olex:present a odrl:Action,
        skos:Concept ;
    skos:inScheme olex:actions ;
    skos:name "present"@en ;
    skos:narrowerThan olex:use .

olex:postOnSocialNetwork a odrl:Action,
        skos:Concept ;
    dc:description """
                    The Assigner permits/prohibits the Assignee(s) to present the content on a social network.
                    """@en ;
    skos:inScheme olex:actions ;
    skos:name "postOnSocialNetwork"@en ;
    skos:narrowerThan olex:present .



olex:Constraint a [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty olex:views ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty olex:height ],
        [ a owl:Restriction ;
            owl:allValuesFrom olex:Host ;
            owl:maxCardinality 1 ;
            owl:onProperty olex:post ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty olex:width ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty olex:attributeText ],
        owl:Class ;
    rdfs:label "Constraint"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf odrl:Constraint .

olex:Purpose a owl:Class ;
    rdfs:label "Purpose"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    owl:deprecated true .

olex:Purposes a skos:ConceptScheme ;
    owl:deprecated true ;
    skos:ConceptScheme_hasTopConcept olex:all_purpose .

olex:all_purpose a olex:Purpose,
