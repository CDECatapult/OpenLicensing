
olex:Region a owl:Class ;
    rdfs:label "Region"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf gn:Feature .

olex:academic_purpose a olex:Purpose,
        skos:Concept ;
    skos:inScheme olex:Purposes ;
    skos:name "academic_purpose"@en ;
    skos:narrowerThan olex:all_purpose .

