
olex:MatchSet a owl:Class ;
    rdfs:label "MatchSet"^^xsd:string ;
    dc:description "A collection described by describing informally what a typical asset element of this set should look like. "@en ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf olex:AssetSet .

ol:hasContributedTo a owl:ObjectProperty ;
    rdfs:domain odrl:Party ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.1/> ;
    rdfs:range olex:Asset ;
    owl:inverseOf ol:contributor .

ol:isTheAuthorOf a owl:ObjectProperty ;
    rdfs:domain odrl:Party ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.1/> ;
    rdfs:range olex:Asset ;
    owl:inverseOf ol:author .

olex:elementType a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain olex:AssetSet ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:range owl:Class .

ol:author a owl:ObjectProperty ;
    dc:description """
        Authors are distinguished of contributors.
        """@en ;
    rdfs:domain olex:Asset ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.1/> ;
    rdfs:range odrl:Party ;
    rdfs:subPropertyOf ol:contributor ;
    owl:inverseOf ol:isTheAuthorOf .

olex:AssetSet a [ a owl:Restriction ;
            owl:allValuesFrom owl:Class ;
            owl:cardinality 1 ;
            owl:onProperty olex:elementType ],
        owl:Class ;
    rdfs:label "AssetSet"^^xsd:string ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf olex:Asset .

ol:contributor a owl:ObjectProperty ;
    dc:description """
        They are assumed necessarily as odrl_Party as they have 
        normally to give their rights at some point if they are contributors.
        """@en ;
    rdfs:domain olex:Asset ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/ol/1.1/> ;
    rdfs:range odrl:Party ;
    rdfs:subPropertyOf dc:creator ;
    owl:inverseOf ol:hasContributedTo .

olex:Asset a [ a owl:Restriction ;
            owl:allValuesFrom odrl:Party ;
            owl:onProperty ol:author ],
        [ a owl:Restriction ;
            owl:allValuesFrom odrl:Party ;
            owl:onProperty ol:contributor ],
        owl:Class ;
    rdfs:label "Asset"^^xsd:string ;
    dc:description """
This is a currently non normative exploratory definition of advanced asset definitions.
"""@en ;
    rdfs:isDefinedBy <http://openlicensing.org/ns/olex/1.0/> ;
    rdfs:subClassOf ol:Asset .

